<%= form_tag("authenticate", method: "get") do %>
  <%= label_tag(:username, "Username:") %>
  <%= text_field_tag(:username) %>
  <%= label_tag(:password, "Password:") %>
  <%= text_field_tag(:password) %>
  <%= submit_tag("Login") %>
<% end %>
<% if @invalid %>
  <h1>Incorrect Username or Password</h1>
<% end %>
<h4>The username is 'admin', no password</h4>
<%= link_to "Add point", login_gen_url %>